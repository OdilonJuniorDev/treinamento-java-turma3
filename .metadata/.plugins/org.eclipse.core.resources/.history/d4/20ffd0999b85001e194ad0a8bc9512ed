package br.com.treinamento;


public class Conta {
	
	

	String agencia;
	int numeroConta;
	double saldo;

	void tranferir() {
		
	}
	
	void sacar(double valor) {
		saldo = saldo - valor;
		
		System.out.println("Saque de : R$" +valor+ " efetuado com sucesso!");
		
	}
	
	void depositar(double valor) { 
		saldo = saldo+valor;
		
		System.out.println("Dep√≥sito de : R$" + valor + "efetuado com sucesso!");
		
	}
	
}
